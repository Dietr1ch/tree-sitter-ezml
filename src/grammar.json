{
  "name": "ezML",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_node"
      }
    },
    "_node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "doctype"
        },
        {
          "type": "SYMBOL",
          "name": "element"
        },
        {
          "type": "SYMBOL",
          "name": "_comment"
        },
        {
          "type": "SYMBOL",
          "name": "_action"
        }
      ]
    },
    "doctype": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "<!"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          },
          {
            "type": "STRING",
            "value": ">"
          }
        ]
      }
    },
    "element": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "element_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "#"
                },
                {
                  "type": "SYMBOL",
                  "name": "element_id_name"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "tag"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "content"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "content": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "element"
              },
              {
                "type": "SYMBOL",
                "name": "_comment"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "element_name": {
      "type": "PATTERN",
      "value": "([A-Za-z_][A-Za-z0-9_-]{0,30})"
    },
    "element_id_name": {
      "type": "PATTERN",
      "value": "([A-Za-z_][A-Za-z0-9_-]{0,30})"
    },
    "_action": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "inline_action"
        },
        {
          "type": "SYMBOL",
          "name": "exec_action"
        }
      ]
    },
    "inline_action": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "<"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "exec_action": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "|"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "tag_name"
        }
      ]
    },
    "tag_name": {
      "type": "PATTERN",
      "value": "([A-Za-z0-9_-]{1,30})"
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "SYMBOL",
          "name": "attribute_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "SYMBOL",
                  "name": "attribute_value"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "attribute_name": {
      "type": "PATTERN",
      "value": "([A-Za-z_][A-Za-z0-9_-]*)"
    },
    "attribute_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "[^\"]*"
              }
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "[^']*"
              }
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        }
      ]
    },
    "_comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "loud_comment"
        },
        {
          "type": "SYMBOL",
          "name": "silent_comment"
        }
      ]
    },
    "loud_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "!"
          },
          {
            "type": "TOKEN",
            "content": {
              "type": "PATTERN",
              "value": ".*"
            }
          }
        ]
      }
    },
    "silent_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/"
          },
          {
            "type": "TOKEN",
            "content": {
              "type": "PATTERN",
              "value": ".*"
            }
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

